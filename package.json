{
  "name": "nostalgie",
  "version": "0.77.3",
  "private": true,
  "description": "Nostalgie is an opinionated, full-stack, runtime-agnostic framework for building web apps and web pages using",
  "main": "index.js",
  "exports": {
    ".": "./src/index.ts",
    "./runtime/client": "./src/runtime/client/index.ts",
    "./plugins/lazy": "./src/runtime/plugins/lazy/plugin.ts",
    "./plugins/lazy/client": "./src/runtime/plugins/lazy/client.ts",
    "./plugins/lazy/server": "./src/runtime/plugins/lazy/server.ts",
    "./lazy": "./src/runtime/plugins/lazy/runtime/index.ts",
    "./plugins/twind": "./src/runtime/plugins/twind/plugin.ts",
    "./plugins/twind/client": "./src/runtime/plugins/twind/client.ts",
    "./plugins/twind/server": "./src/runtime/plugins/twind/server.ts",
    "./runtime/server": "./src/runtime/server/index.ts"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "dev": "SKIP_EMPTY=1 NODE_ENV=development nodemon --watch src --watch scripts/build.js --watch package.json --ext js,jsx,ts,tsx ./scripts/build.js",
    "build": "node ./scripts/build.js",
    "test": "uvu -r esbuild-register",
    "test:watch": "watchlist src tests -- npm run test",
    "posttest": "kacl lint",
    "prepack": "npm run build",
    "preversion": "kacl prerelease",
    "version": "kacl release && git add CHANGELOG.md",
    "postversion": "git push --follow-tags && gh-release"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-transform-react-jsx-self": "^7.12.13",
    "@babel/plugin-transform-react-jsx-source": "^7.12.13",
