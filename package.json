    "@types/hapi__cookie": "^10.1.2",
    "@types/hapi__hapi": "^20.0.8",
    "@types/hapi__inert": "^5.2.2",
    "@types/hapi-pino": "^8.0.1",
    "@types/hast": "^2.3.1",
    "@types/jsesc": "^2.5.1",
    "@types/lru-cache": "^5.1.0",
    "@types/mdx-js__react": "^1.5.3",
    "@types/node": "^12.19.16",
    "@types/pino": "^6.3.4",
    "@types/react": "^16.14.3",
    "@types/react-dom": "^16.6.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/source-map-support": "^0.5.3",
    "@types/unist": "^2.0.3",
    "@types/workerpool": "^6.0.0",
    "@types/yargs": "^16.0.0",
    "@wessberg/rollup-plugin-ts": "^1.3.8",
    "benny": "^3.6.15",
    "esbuild-register": "^2.3.0",
    "gh-release": "^5.0.0",
    "jsdon": "^0.1.14",
    "kacl": "^1.0.0",
    "linkedom": "^0.6.2",
    "lru-cache": "^6.0.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "react-refresh": "^0.9.0",
    "rollup": "^2.38.5",
    "rollup-plugin-dts": "^2.0.1",
    "tailwindcss": "^2.0.2",
    "typescript": "^4.1.3",
    "unified": "^9.2.0",
    "uvu": "^0.5.1",
    "watchlist": "^0.2.3",
    "with-defer": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ggoodman/nostalgie.git"
  },
  "keywords": [
    "react",
    "ssr",
    "react-router",
    "mdx",
    "jamstack",
    "react-query",
    "framework"
  ],
  "author": "Geoffrey Goodman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ggoodman/nostalgie/issues"
  },
  "homepage": "https://github.com/ggoodman/nostalgie#readme",
  "engines": {
    "node": ">=12.13.0",
    "npm": ">7.5.0"
  },
