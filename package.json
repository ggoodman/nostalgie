{
  "name": "nostalgie",
  "version": "0.67.4",
  "private": true,
  "description": "Opinionated template repository for creating a TypeScript library",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "dev": "SKIP_EMPTY=1 nodemon --watch src --watch scripts/build.js --ext js,jsx,ts,tsx ./scripts/build.js",
    "build": "node ./scripts/build.js",
    "posttest": "kacl lint",
    "prepack": "npm run build",
    "preversion": "kacl prerelease",
    "version": "kacl release && git add CHANGELOG.md",
    "postversion": "git push --follow-tags && gh-release"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@types/hapi-pino": "^8.0.1",
    "@types/hapi__cookie": "^10.1.2",
    "@types/hapi__inert": "^5.2.2",
    "@types/lru-cache": "^5.1.0",
    "@types/mdx-js__react": "^1.5.3",
    "@types/node": "^12.19.15",
    "@types/pino": "^6.3.4",
    "@types/react": "^16.6.0",
    "@types/react-dom": "^16.6.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/source-map-support": "^0.5.3",
    "@types/unist": "^2.0.3",
    "@types/workerpool": "^6.0.0",
    "@types/yargs": "^16.0.0",
    "@wessberg/rollup-plugin-ts": "^1.3.8",
    "gh-release": "^5.0.0",
    "kacl": "^1.0.0",
    "lru-cache": "^6.0.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rollup": "^2.38.1",
    "rollup-plugin-dts": "^2.0.1",
    "tailwindcss": "^2.0.2",
    "typescript": "^4.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ggoodman/nostalgie.git"
  },
  "keywords": [
    "react",
    "ssr",
    "react-router",
    "mdx",
    "jamstack",
    "react-query",
    "framework"
  ],
  "author": "Geoffrey Goodman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ggoodman/nostalgie/issues"
  },
  "homepage": "https://github.com/ggoodman/nostalgie#readme",
  "engines": {
    "node": ">=12.13.0"
  },
  "engineStrict": true,
  "prettier": {
    "arrowParens": "always",
    "printWidth": 100,
    "proseWrap": "always",
    "singleQuote": true
  },
  "dependencies": {
    "@antfu/shiki": "^0.6.2",
    "@hapi/boom": "^9.1.1",
    "@hapi/catbox-object": "^2.0.0",
    "@hapi/cookie": "^11.0.2",
    "@hapi/hapi": "^20.0.3",
    "@hapi/inert": "^6.0.3",
    "@hapi/iron": "^6.0.0",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@svgr/core": "^5.5.0",
    "@svgr/plugin-jsx": "^5.5.0",
    "@svgr/plugin-svgo": "^5.5.0",
    "@twind/typography": "0.0.1",
    "abort-controller": "^3.0.0",
    "blob-polyfill": "^4.0.20200601",
    "chokidar": "^3.5.1",
    "esbuild": "^0.8.33",
    "escape-goat": "^3.0.0",
    "escape-string-regexp": "^4.0.0",
    "gray-matter": "^4.0.2",
    "hapi-pino": "^8.3.0",
    "joi": "^17.3.0",
    "openid-client": "^4.2.3",
    "pino": "^6.11.0",
    "pino-pretty": "^4.3.0",
    "piscina": "^2.1.0",
    "react": "^17.0.0 || ^16.8.0",
    "react-dom": "^17.0.0 || ^16.8.0",
    "react-helmet-async": "^1.0.7",
    "react-query": "^3.5.16",
    "react-router-dom": "^5.2.0",
    "remark-autolink-headings": "^5.2.2",
    "remark-github": "^10.0.1",
    "remark-slug": "^5.1.2",
    "resolve": "^1.19.0",
    "source-map-support": "^0.5.19",
    "stacktracey": "^2.0.15",
    "ts-primitives": "^3.2.0",
    "twind": "^0.14.3",
    "unist-util-visit": "^2.0.3",
    "vfile": "^4.2.1",
    "workerpool": "^6.0.4",
    "yargs": "^16.2.0"
  }
}
