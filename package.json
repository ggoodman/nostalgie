{
  "name": "nostalgie",
  "version": "0.77.3",
  "private": true,
  "description": "Nostalgie is an opinionated, full-stack, runtime-agnostic framework for building web apps and web pages using",
  "main": "index.js",
  "bin": "./dist/nostalgie",
  "exports": {
    ".": "./src/index.ts",
    "./config": "./src/config.ts",
    "./data": "./src/plugins/data/runtime/index.ts",
    "./dev": "./src/dev.ts",
    "./lazy": "./src/plugins/lazy/index.ts",
    "./lazy/plugin": "./src/plugins/lazy/plugin/lazyPlugin.ts",
    "./lazy/plugin/client": "./src/plugins/lazy/plugin/client.ts",
    "./lazy/plugin/server": "./src/plugins/lazy/plugin/server.ts",
    "./markup": "./src/plugins/markup/index.ts",
    "./markup/plugin": "./src/plugins/markup/plugin/plugin.ts",
    "./markup/plugin/client": "./src/plugins/markup/plugin/client.ts",
    "./markup/plugin/server": "./src/plugins/markup/plugin/server.ts",
    "./routing": "./src/plugins/routing/index.ts",
    "./routing/plugin": "./src/plugins/routing/plugin/routingPlugin.ts",
    "./routing/plugin/client": "./src/plugins/routing/plugin/client.ts",
    "./routing/plugin/server": "./src/plugins/routing/plugin/server.ts",
    "./twind": "./src/plugins/twind/runtime/index.ts",
    "./twind/plugin": "./src/plugins/twind/plugin.ts",
    "./twind/plugin/client": "./src/plugins/twind/client.ts",
    "./twind/plugin/server": "./src/plugins/twind/server.ts",
    "./twind/plugin/shim/client": "./src/plugins/twind/shim/client.ts",
    "./twind/plugin/shim/server": "./src/plugins/twind/shim/server.ts",
    "./internal/client": "./src/internal/client/index.ts",
    "./internal/server": "./src/internal/server/index.ts"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "dev": "SKIP_EMPTY=1 NODE_ENV=development nodemon --watch src --watch scripts/build.js --watch package.json --ext js,jsx,ts,tsx ./scripts/build.js",
    "build": "node ./scripts/build.js",
    "test": "vitest",
    "test:watch": "watchlist src tests -- npm run test",
    "posttest": "kacl lint",
    "prepack": "npm run build",
    "preversion": "kacl prerelease",
    "version": "kacl release && git add CHANGELOG.md",
    "postversion": "git push --follow-tags && gh-release"
  },
  "devDependencies": {
    "@emotion/is-prop-valid": "^1.1.2",
    "@ggoodman/context": "^2.1.0",
    "@mdx-js/mdx": "^2.1.1",
    "@n1ru4l/push-pull-async-iterable-iterator": "^3.2.0",
    "@types/debug": "^4.1.5",
    "@types/jsesc": "^3.0.1",
    "@types/node": "^16.11.26",
    "@types/pino": "^7.0.5",
    "@types/react": "^18.0.1",
    "@types/react-dom": "^18.0.0",
    "@types/yargs": "^17.0.10",
    "@vitejs/plugin-react": "^1.3.0",
    "@wessberg/rollup-plugin-ts": "^2.0.4",
    "chokidar": "^3.5.3",
    "debug": "^4.3.1",
    "esbuild": "^0.14.34",
    "esbuild-register": "^3.3.2",
    "escape-string-regexp": "^5.0.0",
    "fast-glob": "^3.2.11",
    "fastify": "^3.28.0",
    "gh-release": "^5.0.0",
    "git-remote-origin-url": "^4.0.0",
    "happy-dom": "^2.55.0",
    "headers-utils": "^3.0.2",
    "jsdon": "^0.1.14",
    "jsesc": "^3.0.2",
    "kacl": "^1.0.0",
    "linkedom": "^0.14.5",
    "magic-string": "^0.26.1",
    "nodemon": "^2.0.7",
    "nostalgie": "file:dist",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "react-dev-inspector": "^1.8.0",
    "react-error-boundary": "^3.1.4",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "remark-frontmatter": "^4.0.1",
    "remark-mdx-frontmatter": "^1.1.1",
    "rollup": "^2.38.5",
    "rollup-plugin-dts": "^4.2.1",
    "runtypes": "^6.0.0",
    "source-map": "^0.7.3",
    "twind": "^0.16.10",
    "typescript": "^4.6.3",
    "uvu": "^0.5.1",
    "vite": "^2.7.3",
    "vitest": "^0.10.0",
    "yargs": "^17.4.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ggoodman/nostalgie.git"
  },
  "keywords": [
    "react",
    "ssr",
    "react-router",
    "mdx",
    "jamstack",
    "react-query",
    "framework"
  ],
  "author": "Geoffrey Goodman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ggoodman/nostalgie/issues"
  },
  "homepage": "https://github.com/ggoodman/nostalgie#readme",
  "engines": {
    "node": ">=12.13.0",
    "npm": ">7.5.0"
  },
  "engineStrict": true,
  "prettier": {
    "arrowParens": "always",
    "printWidth": 80,
    "proseWrap": "always",
    "singleQuote": true
  },
  "dependencies": {
    "esbuild": "^0.14.34"
  },
  "workspaces": [
    "./packages/create-nostalgie-app",
    "./packages/template-javascript",
    "./packages/template-typescript"
  ],
  "volta": {
    "node": "16.14.2",
    "npm": "8.6.0"
  }
}
