    "@types/react-dom": "16",
    "@types/react-router-dom": "5",
    "@types/react-side-effect": "^1.1.1",
    "@types/unist": "^2.0.3",
    "@types/workerpool": "^6.0.0",
    "@wessberg/rollup-plugin-ts": "^1.3.8",
    "abort-controller": "^3.0.0",
    "dts-bundle-generator": "^5.6.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rollup": "^2.36.2",
    "tailwindcss": "^2.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ggoodman/typescript-library-template.git"
  },
